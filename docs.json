{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "almond",
  "name": "Mycelium SDK",
  "colors": {
    "primary": "#6F56F9",
    "light": "#7E9EFF",
    "dark": "#445198"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "global": {
      "anchors": [
        {
          "anchor": "Documentation",
          "icon": "book-open-cover",
          "href": "https://mintlify.com/docs"
        }
      ]
    },
    "tabs": [
      {
        "tab": "Overview",
        "groups": [
          {
            "group": "Getting started",
            "pages": [
              "welcome",
              "quickstart",
              "api-key",
              "protocols",
              "support"
            ]
          },
          {
            "group": "Versions",
            "pages": [
              "overview/versions/overview",
              {
                "group": "Cloud",
                "pages": [
                  "versions/cloud",
                  "overview/versions/cloud/untitled-page"
                ]
              },
              {
                "group": "Self-hosted",
                "pages": [
                  "versions/self-hosted/overview",
                  "versions/self-hosted/installation",
                  "versions/self-hosted/variables",
                  {
                    "group": "Testing",
                    "pages": [
                      "versions/self-hosted/cli",
                      "versions/self-hosted/cli-env-variables",
                      "overview/versions/self-hosted/testing/local-faucet"
                    ]
                  }
                ]
              }
            ]
          },
          {
            "group": "Gas sponsoring",
            "pages": [
              "overview/gas-sponsoring/untitled-page",
              "overview/gas-sponsoring/use-paymaster"
            ]
          },
          {
            "group": "About Mycelium",
            "pages": [
              "about-mycelium/what-is",
              "about-mycelium/team"
            ]
          }
        ]
      },
      {
        "tab": "SDK reference",
        "hidden": true,
        "groups": [
          {
            "group": "API documentation",
            "pages": [
              "api-reference/introduction"
            ]
          },
          {
            "group": "Endpoint examples",
            "pages": [
              "api-reference/endpoint/get",
              "api-reference/endpoint/create",
              "api-reference/endpoint/delete",
              "api-reference/endpoint/webhook"
            ]
          }
        ]
      }
    ]
  },
  "logo": {
    "light": "/logo/logo-long-light.png",
    "dark": "/logo/logo-long-dark.png"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:myceliumsdk@gmail.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://dashboard.mintlify.com"
    }
  },
  "contextual": {
    "options": [
     "copy",
     "view",
     "chatgpt",
     "claude",
     "perplexity",
     "mcp",
     "cursor",
     "vscode"
   ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/mycelium_sh",
      "github": "https://github.com/MyceliumSDK/mycelium-sdk",
      "website": "https://mycelium.sh/"
    }
  },
  "description": "Gateway to advanced web3 yield",
  "appearance": {
    "default": "light"
  }
}
